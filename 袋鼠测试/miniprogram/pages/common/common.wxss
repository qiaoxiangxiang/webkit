.buyshopping_xuanBox{
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
}
.buyshopping_xuan{
  width: 46%;
  min-height: 66.533rpx;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  border: 0.6rpx solid #999;
  padding: 0 1%;
