/* 公用样式 */
page{
  width: 100%;height: 100%;
}
image{
  width: 0;
  height: 0;
  display: block;
}
.content{
  width: 100%;
  position: relative;
}
.navigator-hover {
  background: rgba(0,0,0,0);
  opacity: 1;
}
.listofcourse_content,.listofcourse_topTxt,.listofcourse_liebiaoBox {
  width:689.535rpx;
  margin:0 auto;
}

/* 课程列表 */
.listofcourse_zeng,.listofcourse_quan{
  padding: 4rpx 6rpx;
  background: #ff8c1c;
  font-size: 21.169rpx;
  color: #fff;
  border-radius: 7rpx;
  margin-left: 12rpx;
}

.listofcourse_ke{
  width: 100%;
  height: 200.807rpx;
  background: #fff;
  box-shadow: 0rpx 0rpx 40rpx #f3f3f3;
  border-radius: 20rpx;
  margin-bottom: 18.148rpx; 
  padding-top: 20.568rpx;
  display: flex;
  justify-content: space-between;
  position: relative;
}
.listofcourse_ke0{
  margin-top: 18.148rpx;
}
.listofcourse_keImg{
  width: 281.860rpx;
  margin-left: 12.097rpx; 
}
.listofcourse_kecontent{
  width: 381.056rpx;
  margin-left: 15.726rpx;
}
.listofcourse_banName{
  font-size: 26.032rpx;
  font-weight: 600;
  color: #333;
  display: flex;
  align-items: center;
}
.listofcourse_keName,.listofcourse_ketime{
  font-size: 21.169rpx;
  color: #999;
  margin-top: 9.677rpx;
}
.listofcourse_kePrice{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-top: 9.677rpx;
}
.listofcourse_Prices{
