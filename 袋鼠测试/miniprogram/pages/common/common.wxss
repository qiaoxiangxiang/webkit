}
.buyshopping_isxuan{
  background: #e8f1fc;
  color: #227ae1;
  border-color: #227ae1;
}
.buyshopping_xuanTxt,.buyshopping_xuanPrice{
  font-size: 24.198rpx;
  color: #999;
}
.buyshopping_isxuan .buyshopping_xuanTxt{
  color: #227ae1;
}
.buyshopping_xuanPrice{
  color: #e83121;
}
.buyshopping_bottom{
  position: absolute;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 95.566rpx;
  box-shadow:0rpx -6rpx 25rpx #eee;

}
.buyshopping_goubuyPrice,.buyshopping_goubuyche,.buyshopping_goubuybuy{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 34%;
  height: 100%;
  font-size: 27.218rpx;
  color: #fff;
}
.buyshopping_goubuyImgs{
  width: 29.032rpx;
  margin-right: 12rpx;
}
.buyshopping_priceTxt{
  font-size: 29.032rpx;
  color: #333;
}
.buyshopping_priceNum{
  font-size: 34.291rpx;
  color: #e83121;
}
.buyshopping_goubuyche{
  background: #387fff;
}
.buyshopping_goubuybuy{
  background: #ff7e00;
}
.two_year{
  font-size: 27.218rpx;
