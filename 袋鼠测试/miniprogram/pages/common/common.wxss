  margin: 0 auto;
}
.buyshopping_top{
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.buyshopping_txt{
  display: flex;
  align-items: center;
}
.buyshopping_title{
  font-size: 36.291rpx;
