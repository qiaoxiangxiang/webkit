  justify-content: space-between;
  align-items: center; 
  padding-top: 26rpx;
}
.shop_title{
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 33.266rpx;
  color: #333;
}
.shop_title_xian{
  width: 6.04rpx;
  height: 38.062rpx;
  border-radius: 6.04rpx;
  background: #008cdc;
